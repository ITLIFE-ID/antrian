<%=tag.div id: data[:element]%>
<script>
  var colors = "<%=data[:colors]%>".split(",")
   Highcharts.chart(<%=data[:element]%>, {
     chart: {
       plotBackgroundColor: null,
       plotBorderWidth: null,
       plotShadow: false,
       type: 'pie'
     },
     title: {
       text: '<%=data[:element].to_s.humanize%>'
     },
     tooltip: {
       pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
     },
     accessibility: {
       point: {
           valueSuffix: '%'
       }
     },
     plotOptions: {
       pie: {
        colors: colors,
         allowPointSelect: true,
         cursor: 'pointer',
         dataLabels: {
           enabled: true,
           format: '<b>{point.name}</b>: {point.percentage:.1f} %',
           connectorColor: 'silver'
         }
       }
     },
     series: [{
       name: 'Share',
       data: <%=data[:data].to_json.html_safe%>
     }]
   });
</script>
